package trefethen.talk.gui;


public class ScreenLogin extends Screen {
	
	public ScreenLogin() {
		
	}

	@Override
	public void onOpen() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void onEnable() {
		GUIManager.contentPanel.add(GUIFactory.wrap(GUIFactory.createTextField()));
		GUIManager.contentPanel.add(GUIFactory.wrap(GUIFactory.createTextField()));
		GUIManager.contentPanel.add(GUIFactory.wrap(GUIFactory.createButton()));
		GUIManager.contentPanel.add(GUIFactory.wrap(GUIFactory.createButton()));
	}

	@Override
	public void onDisable() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void onClose() {
		// TODO Auto-generated method stub 
		
	}

}

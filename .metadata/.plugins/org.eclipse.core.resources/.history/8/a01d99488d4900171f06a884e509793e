package trefethen.talk.gui;

import java.awt.BorderLayout;

import javax.swing.JLabel;

public class Screen {
	
	public String name;
	
	public Screen(String name) {
		this.name = name;
	}
	
	public void onOpen() {
		onEnable();
	}
	
	public void onEnable() {
		GUIManager.setNavBar(name);
	}
	
	public void onDisable() {
		
	}
	
	public void onClose() {
		
	}

}

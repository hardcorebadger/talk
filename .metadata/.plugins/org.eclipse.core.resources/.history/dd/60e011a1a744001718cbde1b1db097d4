		name = n;
		id = i;
		messages = new ArrayList<Message>();
	}
	
	public Chat(JSONObject obj) {
	}
	
	public JSONObject toJSON() {
		return null;
	}
	
	public int getID() {
		return id;
	}
	
	public void display() {
		
	}

}

import org.json.simple.JSONObject;

import trefethen.talk.user.User;;

public class Message {
	
	private User sender;
	private String message;
	
	public Message(User u, String m) {
		sender = u;
		message = m;
	}
	
	public Message(JSONObject obj) {
		
	}
	
	public JSONObject toJSON() {
		return null;
	}

}
